<div class="container">
<div class="row">
<div class="col-md-3">
<%= render 'users/list', user: @user %>
  </div>
  <div class="col-md-8 offset-md-1">
<h1>Users</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <th>image</th>
            <th>name</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
         <% @users.each do |user| %>
        <tr>
            <td><%= attachment_image_tag user, :profile_image, size:"60x60", fallback: "PngItem_1503945.png" %></td>
            <td><%= user.name %></td>
            <td><%= link_to "Show", user_path(user.id) %></td>
        </tr>
        <% end %>
    </tbody>
</table>
</div>
</div>  <!-- この行を追加 -->
</div>